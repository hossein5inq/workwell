{
  "name": "workwell-v2",
  "version": "1.0.21",
  "description": "Workwell JS is the SDK to communicate between a web-application and the Workwell mobile application",
  "main": "dist/js/workwell.js",
  "scripts": {
    "watch": "npm run css && rollup -c --watch",
    "css": "node-sass sass -o dist/css",
    "css-watch": "node-sass --watch sass -o dist/css",
    "js": "rollup -c",
    "js-watch": "rollup -c --watch",
    "build": "npm run css && rollup -c && NODE_ENV=production rollup -c"
  },
  "keywords": [
    "workwell",
    "sdk",
    "framework",
    "sass"
  ],
  "devDependencies": {
    "animejs": "^2.2.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015-rollup": "^3.0.0",
    "chai": "^3.5.0",
    "node-sass": "^4.9.0",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.1",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-watch": "^4.3.1",
    "sinon": "^1.17.7",
    "velocity-animate": "1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Workwell/workwell.git"
  }
}
